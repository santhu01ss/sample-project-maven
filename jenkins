pipeline {
    agent any 
        stages {
            stage ('git'){
                steps {
                    sh 'echo hi git pipeline'
                    echo 'hi hlo'
                }
            }
        stage ('maven'){
            steps{
                sh 'echo maven pipeline'
                echo 'hlo maven'
            }
        }
        stage ('sonar'){
            steps{
                sh 'echo sq pipeline'
            }
        }
        stage('nexus'){
            steps{
                sh 'echo nexus pipeline'
            }
        }
        }
        
}
